{
    "actions": [
        {"type": "Label", "caption": "Import & export of ..." },
        {
            "type": "RowLayout",
            "items": [
                {
                    "type": "PopupButton",
                    "caption": "ICS File",
                    "popup": {
                        "caption": "Import & export Waste data for Kleve ...",
                        "items": [
                            {
                                "type": "RowLayout",
                                "items": [
                                    {"type": "SelectFile", "name": "FileWaste", "caption": "File:", "extensions": ".ics"},
                                    {"type": "Button", "caption": "Import", "onClick": "IPS_RequestAction($id,'OnImportWaste',$FileWaste);"},
                                    {"type": "Button", "caption": "Export", "link": true, "onClick": "echo \"../hook/waste$id?export=BD\";"},
                                    {"type": "Button", "caption": "Clear", "onClick": "IPS_RequestAction($id,'OnDeleteDays','BD');"}
                                ]
                            },
                            {"type": "Label", "caption": "ATTENTION: Clearing the list deletes all entries without asking!"},
                            {"type": "Label", "caption": "NOTE: No file needs to be selected for export!"}
                        ]
                    }
                }
            ]
        },
        {"type": "Label",  "caption": "Date info ..." },
        {"type": "Button", "caption": "Update", "onClick": "WASTE_Update($id);"},
        {"type": "Label",  "caption": "\n" },
        {
            "type": "ExpansionPanel",
            "caption": "Source code, donation and licence ...",
            "items": [
                {"type": "Label", "caption": "The software is free of charge for non-commercial use, I would appreciate a donation if you like the module.", "bold": true},
                {
                    "type": "RowLayout",
                    "items": [
                        {"type": "Image", "image": "data:image/svg+xml;base64,", "onClick": "echo 'https://wilkware.github.io/';"},
                        {"type": "Image", "image": "data:image/svg+xml;base64,", "onClick": "echo 'https://www.paypal.com/cgi-bin/webscr?cmd=_s-xclick&hosted_button_id=8816166';"},
                        {"type": "Image", "image": "data:image/svg+xml;base64,", "onClick": "echo 'https://creativecommons.org/licenses/by-nc-sa/4.0/';"}
                    ]
                }
            ]
        }
    ]
}